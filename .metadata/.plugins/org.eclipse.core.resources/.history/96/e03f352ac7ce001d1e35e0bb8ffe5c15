package ex04.For;

public class MainEntry {
	public static void main(String[] args) {
		
		//for ( int i = 1; i < 6; i++ ) {	
		//	System.out.print("*"); //*****
			
		for ( int i = 1; i <= 10; i++ ) {	
			System.out.print(i + " "); //1~10	
		}
	}
}
