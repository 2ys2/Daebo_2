package Quiz;

import java.util.Scanner;

public class homework01 {
	
	public static void main(String[] args) {
		
	
	
	Scanner sc = new Scanner(System. in);
	
	int A;
	//int B;
	
	A = sc.nextInt();
	//B = sc.nextint();
	
	int A_100 = A / 100;
	int A_10 = (A % 100) / 10;
	int A_1 = A_10 / 1;
	
	System.out.println(A_100 + " " +  A_10 + " " + A_1);
	
	}
	
	
}
