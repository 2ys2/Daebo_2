package Quiz;

import java.util.Scanner;

public class Quiz_Switch {
	public static void main(String[] args) {
		System.out.println("당신이 받은 점수는? ");
		int jumsu = new Scanner(System.in).nextInt();
		
		//평점(학점 프로그램 작성

		
		switch ( jumsu ) {
			case ( jumsu <= 100 && jumsu > 89 ) :
				System.out.print("A");
				break;
			case ( jumsu <= 90 && jumsu > 79 ) :
				System.out.print("B");
				break;
			case ( jumsu <= 80 && jumsu > 69 ) :
				System.out.print("C");
				break;
			case ( jumsu <= 70 && jumsu > 59 ) :
				System.out.print("D");
				break;
			case ( jumsu < 59 ) :
				System.out.print("F");
				break;
				
			//default :
		}
		
		System.out.println(" 학점 입니다.");
	}
}
