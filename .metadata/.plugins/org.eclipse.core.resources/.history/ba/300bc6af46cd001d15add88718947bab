package Quiz;

import java.util.Scanner;

public class homework01 {
	
	public static void main(String[] args) {
		
	
	
	Scanner sc = new Scanner(System. in);
	
	int A;
	int B;
	
	A = sc.nextInt();
	B = sc.nextInt();
	
	int A_100 = A / 100;
	int A_10 = (A % 100) / 10;
	int A_1 = (A % 100) % 10;
	
	int B_100 = B / 100;
	int B_10 = (B % 100) / 10;
	int B_1 = (B % 100) % 10;
	
	System.out.println(A_100 + " " +  A_10 + " " + A_1);
	System.out.println(B_100 + " " +  B_10 + " " + B_1);
	
	}
	
	
}
